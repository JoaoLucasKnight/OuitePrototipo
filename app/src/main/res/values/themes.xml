<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.OuitePrototipo" parent="Theme.Material3.Light.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
<!--Background-->
        <item name="android:colorBackground">@color/branco</item>
<!--Outline -->
        <item name="colorOutline">@color/azul_m</item>
        <item name="colorOutlineVariant">@color/azul_b</item>

<!--Surfaces-->
        <item name="colorSurfaceVariant">@color/azul_m</item>
        <item name="colorSurfaceDim">@color/azul_m</item>
        <item name="colorSurface">@color/marron</item>
        <item name="colorOnSurface">@color/azul_b</item>

        <item name="colorSurfaceBright">@color/branco</item>

<!--Primary-->
        <item name="colorPrimary">@color/marron</item>
        <item name="colorOnPrimary">@color/branco</item>
        <item name="colorPrimaryContainer">@color/marron_m</item>
        <item name="colorOnPrimaryContainer">@color/marron_b</item>

<!--Secondary-->
        <item name="colorSecondary">@color/azul</item>
        <item name="colorOnSecondary">@color/preto</item>
        <item name="colorSecondaryContainer">@color/azul_m</item>
        <item name="colorOnSecondaryContainer">@color/azul_b</item>

<!--Erro-->
        <item name="colorError">@color/red_m</item>
        <item name="colorOnError">@color/branco</item>
        <item name="colorErrorContainer">@color/red</item>
        <item name="colorOnErrorContainer">@color/red_b</item>
    </style>

    <style name="Theme.OuitePrototipo" parent="Base.Theme.OuitePrototipo" />
</resources>

